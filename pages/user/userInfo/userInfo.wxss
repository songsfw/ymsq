page{background: #F6F6F6;height: auto;}
.def-txt{color: #aaa;}
.address{position: relative; background: #fff url(data:image/jpg;base64,) no-repeat center top;background-size: contain; padding:0 20rpx;margin:114rpx 24rpx 24rpx 24rpx;border-radius: 16rpx;padding-top: 150rpx;}
.inputxt{display: flex; border-bottom: 1px solid #F6F6F6;height: 49px;line-height: 49px;align-items: center;color: #222;font-size: 15px;}
.inputxt>text{display: block;width:50px ; color: #222;font-size: 14px;margin-right: 20px;flex-shrink: 0;}
.inputxt input{height: 100%;color: #222;font-weight: 500;font-size: 15px;}
.inputxt .name{font-size: 15px;}
.checkbox{position: relative; width: 18px;height: 18px;border: 1px solid #ccc;border-radius: 50%;line-height: 1;margin-right: 20rpx;box-sizing: border-box;}
.checkbox view{position: absolute;left: 0;top: 0; display: none; width: 100%;height: 100%;font-size: 18px;color:#C1996B;}
.checkbox.on{border: none;}
.checkbox.on view{display: block;}
.default{color: #999;}
.default.checked{color: #C1996B;}

.level{width:51px;height: 21px;margin-top: 10rpx;}
.level-4{background-position:100% 0;}
.level-3{background-position:100% -55px;}
.level-0{background-position:100% -29px;}
.item{justify-content: space-between;font-weight: 500;}
.icon-arrow-r{color: #999;}
.userInfo{position: absolute;left: 50%;transform: translateX(-50%);top:-84rpx;}
.userInfo .name{display: block; font-size: 18px;margin: 20rpx auto 0 auto; text-align: center;font-weight: 500;}
.photo{position: relative; width: 150rpx;height: 150rpx;margin: 0 auto;}
.photo open-data{display: block; width: 100%;height: 100%;overflow: hidden;border-radius: 50%;}
.photo .level{position: absolute;right:-25px ;bottom: 0;}

.sex text{font-size: 15px;}
.sex view{position: relative; width:14px;height: 14px;border: 1px solid #999;border-radius: 50%;margin-right:10px ;}
.sex-item view:last-child{margin-left: 50px;}
.sex view:after{position: absolute;content: "";width: 8px;height: 8px;background-color: #FF7A7A;left:3px ;top: 3px;border-radius: 50%;transition: all .2s linear;transform:scale(0);}
.sex.on view{border-color: #FF7A7A;}
.sex.on{color: #FF7A7A;}
.sex.on view:after{display: block; transform:scale(1)}

.btn2{width: 72px;height: 24px;line-height: 24px;text-align: center; border-radius: 2px;border:1px solid #C1996B ;font-size: 13px; color: #C1996B;flex-shrink: 0;}

.sex.on.man view{border-color: #4176FF;}
.sex.on.man{color: #4176FF;}
.sex.man view:after{background-color: #4176FF;}
.sex.on.man view:after{display: block; transform:scale(1)}
.picker text{display: block; height: 49px;}